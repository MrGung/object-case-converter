{
  "name": "object-case-converter",
  "version": "1.0.4",
  "description": "Convert keys in an javascript object for some cases(camelCase, snake_case, etc.)",
  "main": "dist/index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "jest --forceExit",
    "lint": "eslint ./src",
    "build": "webpack"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "coverageReporters": [
      "clover",
      "html"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ridibooks/object-case-converter.git"
  },
  "keywords": [
    "converter",
    "camelcase",
    "snakecase"
  ],
  "author": "Kyungmi Koong <kyungmi.k@ridi.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ridibooks/object-case-converter/issues"
  },
  "homepage": "https://github.com/ridibooks/object-case-converter#readme",
  "dependencies": {
    "lodash.camelcase": "^4.3.0",
    "lodash.snakecase": "^4.1.1"
  },
  "devDependencies": {
    "@ridi/eslint-config": "*",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "eslint": "^4.4.0",
    "jest": "^21.0.0",
    "webpack": "^3.4.1"
  }
}
